    <script>
        $(function() {
            $('#userRole').on('change',function(){
                if( $(this).val()==="instructor" ||$(this).val()==="student" ){
                    $("#admin").show()
                }
                else{
                    $("#admin").hide()
                }
            });
        });
        $(function() {
            $('#userRole').on('change',function(){
                if( $(this).val()==="student"){
                    $("#student").show()
                }
                else{
                    $("#student").hide()
                }
            });
        });
    </script>
